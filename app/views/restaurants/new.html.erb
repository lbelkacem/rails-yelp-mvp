<%= simple_form_for (@restaurant) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :category, collection: Restaurant::TYPES %>
    <%= f.input :phone_number %>
    <%= f.input :address %>
  </div>

  <div class="form-actions">
      <%= f.button :submit %>
  </div>
<% end %>
